local function redeemCode(code)
    local args = {
        [1] = code
    }
    local success, errorMsg = pcall(function()
        game:GetService("ReplicatedStorage").MainEvent:FireServer("EnterPromoCode", unpack(args))
    end)
    if success then
        print("Codice riscattato: " .. code)
    else
        print("Errore con il codice: " .. code .. " - " .. errorMsg)
    end
end

for _, code in ipairs(codes) do
    redeemCode(code)
    wait(5) -- Aumenta l'attesa se necessario
end
end})

